{
  "name": "hackeddeeeepio",
  "productName": "Hacked Deeeep.io",
  "version": "1.0.0",
  "description": "Blok fis gaem v3 client",
  "main": ".webpack/main",
  "build": {
    "appId": "com.pi.hackeddeeeepio",
    "productname": "Hacked Deeeep.io Desktop Client",
    "win": {
      "icon": "build/icon.ico"
    },
    "squirrelWindows": {
      "iconUrl": "https://github.com/ThePiGuy3141/DeeeepClient/blob/hacked/icon.ico?raw=true",
      "setupIcon": "build/icon.ico"
    }
  },
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": {
    "name": "pi"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "build/icon.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "HackedDeeeepioClient",
            "iconUrl": "https://github.com/ThePiGuy3141/DeeeepClient/blob/hacked/icon.ico?raw=true",
            "setupIcon": "build/icon.ico",
            "loadingGif": "build/install.gif"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-wix": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@electron-forge/plugin-webpack": "6.0.0-beta.63",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.0",
    "css-loader": "^6.0.0",
    "electron": "^18.2.0",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "fork-ts-checker-webpack-plugin": "^6.0.1",
    "node-loader": "^2.0.0",
    "style-loader": "^3.0.0",
    "toml": "^3.0.0",
    "ts-loader": "^9.2.2",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "bulma": "^0.9.4",
    "electron-squirrel-startup": "^1.0.0",
    "git": "^0.1.5"
  }
}
